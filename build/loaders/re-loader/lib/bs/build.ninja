bsc = C:\Users\Shin\AppData\Roaming\nvm\v7.7.3\node_modules\bs-platform\lib\bsc.exe
bsdep = C:\Users\Shin\AppData\Roaming\nvm\v7.7.3\node_modules\bs-platform\lib\bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101
bsc_flags = -nostdlib -I "C:\Users\Shin\Desktop\reason-vue\node_modules\bs-platform\lib\ocaml" -bs-no-version-header -no-alias-deps -color always
ppx_flags = 
bs_package_includes = 
bs_package_dev_includes = 
namespace = 
bsb_dir_group = 0
refmt = C:\Users\Shin\AppData\Roaming\nvm\v7.7.3\node_modules\bs-platform\lib\refmt.exe
reason_react_jsx = 
refmt_flags = --print binary
bsc_lib_includes = -I src-cloned\components -I src-cloned
rule build_ast_and_module_sets_from_re
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${warnings} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src-cloned\App.mlast : build_ast_and_module_sets_from_re $src_root_dir\src-cloned\App.re
rule build_deps
  command = ${bsdep} ${namespace} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src-cloned\App.mlast.d : build_deps src-cloned\App.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src-cloned\App.cmj |  $src_root_dir\src-cloned\App.js src-cloned\App.cmi : build_cmj_cmi src-cloned\App.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src-cloned
build src-cloned\vue.mlast : build_ast_and_module_sets_from_re $src_root_dir\src-cloned\vue.re
build src-cloned\vue.mlast.d : build_deps src-cloned\vue.mlast
build src-cloned\vue.cmj |  $src_root_dir\src-cloned\vue.js src-cloned\vue.cmi : build_cmj_cmi src-cloned\vue.mlast
