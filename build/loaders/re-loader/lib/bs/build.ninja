build src-cloned\main.mlast.d : build_deps src-cloned\main.mlast
build src-cloned\main.cmj |  $src_root_dir\src-cloned\main.js src-cloned\main.cmi : build_cmj_cmi src-cloned\main.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src-cloned
build src-cloned\components\HelloWorld.mlast : build_ast_and_module_sets_from_re $src_root_dir\src-cloned\components\HelloWorld.re
build src-cloned\components\HelloWorld.mlast.d : build_deps src-cloned\components\HelloWorld.mlast
build src-cloned\components\HelloWorld.cmj |  $src_root_dir\src-cloned\components\HelloWorld.js src-cloned\components\HelloWorld.cmi : build_cmj_cmi src-cloned\components\HelloWorld.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
