bs_package_flags = -bs-package-name reason-vue
src_root_dir = /Users/jasons/Desktop/reason-vue
bsc = /Users/jasons/.nvm/versions/node/v7.7.3/lib/node_modules/bs-platform/lib/bsc.exe
bsdep = /Users/jasons/.nvm/versions/node/v7.7.3/lib/node_modules/bs-platform/lib/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101
bsc_flags = -bs-suffix -nostdlib -I '/Users/jasons/Desktop/reason-vue/node_modules/bs-platform/lib/ocaml' -bs-no-version-header -no-alias-deps -color always
ppx_flags = 
bs_package_includes = 
bs_package_dev_includes = 
namespace = 
bsb_dir_group = 0
refmt = /Users/jasons/.nvm/versions/node/v7.7.3/lib/node_modules/bs-platform/lib/refmt3.exe
reason_react_jsx = 
refmt_flags = --print binary
bsc_lib_includes = -I src
rule build_ast_and_module_sets_from_re
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${warnings} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/vue.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/vue.re
rule build_deps
  command = ${bsdep} ${namespace} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/vue.mlast.d : build_deps src/vue.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/vue.cmj |  $src_root_dir/src/vue.bs.js src/vue.cmi : build_cmj_cmi src/vue.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src
build src/main.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/main.re
build src/main.mlast.d : build_deps src/main.mlast
build src/main.cmj |  $src_root_dir/src/main.bs.js src/main.cmi : build_cmj_cmi src/main.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src
build build.ninja : phony  ||  src/main.mlast.d src/vue.mlast.d
